# Dosfet internal PV names relate to the channels on the box.
# In order to maintain the relationship between internal PVs while giving
# dose values names related to accelerator location, create a set of
# aliases mapping user/accelerator PV names to internal ones.

record( ai, "$(USER_P)1:RAW")
{
	field( INP, "$(P):$(SENSOR):1:RAW CPP")
}

record( ai, "$(USER_P)1:ZERO_VOLTS")
{
	field( INP, "$(P):$(SENSOR):1:ZERO_VOLTS CPP")
}

record( bo, "$(USER_P)1:ZERO")
{
	field( OUT, "$(P):$(SENSOR):1:ZERO CPP")
}

record( stringin, "$(USER_P)1:ZERO_TOD")
{
	field( INP, "$(P):$(SENSOR):1:ZERO_TOD CPP")
}

