}

record( mbbi, "$(USER_P)1:STATUS")
{
	field( INP, "$(P):$(SENSOR):1:RAW_STATUS CPP")
	field( ZRVL, "0" )
	field( ONVL, "1" )
	field( TWVL, "2" )
	field( ZRST, "Ok" )
	field( ONST, "Over Linear Range" )
	field( TWST, "Error")
	field( ZRSV, "NO_ALARM" )
	field( ONSV, "MINOR" )
	field( TWSV, "INVALID" )
}

