record(bo, "K2612B:StartRead")
{
  field(DESC, "Serial No of SMU")
  field(DTYP, "stream")
  field(OUT,  "@K2612B_RadFET.proto StartRead $(PORT)")
  field(SCAN, "10 second")
}

